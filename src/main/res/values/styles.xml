    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#00bcd4</item>
        <item name="colorPrimaryDark">@color/style_color_primary_dark</item>
        <item name="colorAccent">@color/theme_accent</item>
    </style>

    <style name="Theme.DesignDemo" parent="Base.Theme.DesignDemo"></style>

    <style name="Base.Theme.DesignDemo" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#00bcd4</item>
        <item name="colorPrimaryDark">#512DA8</item>
        <item name="colorAccent">#00bcd4</item>
        <item name="android:windowBackground">@color/window_background</item>
    </style>

    <style name="Widget.CardContent" parent="android:Widget">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">24dp</item>
        <item name="android:orientation">vertical</item>
    </style>


    <style name="AppTheme.TransparentActivity">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.TransparentActivity.FullScreen">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="ContextMenuButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_context_menu</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="MaterialSettingsTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="msColorPrimary">@color/primary</item>
        <item name="msColorPrimaryDark">@color/primary_dark</item>
        <item name="msColorAccent">@color/accent</item>
        <item name="msWindowBg">@color/ms_white</item>
        <item name="msToolbarTheme">@style/MaterialSettingsTheme_Toolbar</item>
        <item name="msToolbarThemePopup">@style/MaterialSettingsTheme_Toolbar_Popup</item>
        <item name="msDivider">@style/MaterialSettingsTheme_Divider</item>
        <item name="msColorHeader">@color/accent</item>
        <item name="msColorTextTitle">@color/ms_black</item>
        <item name="msColorTextSubTitle">@color/ms_gray_dark</item>
        <item name="msTextStyle">@style/MaterialSettingsTheme_Text</item>
        <item name="msSubTextStyle">@style/MaterialSettingsTheme_SubText</item>
        <item name="msHeaderStyle">@style/MaterialSettingsTheme_Header</item>
        <item name="msItem">@style/MaterialSettingsTheme_Item</item>
        <item name="android:windowBackground">?msWindowBg</item>
        <item name="colorPrimary">?msColorPrimary</item>
        <item name="colorPrimaryDark">?msColorPrimaryDark</item>
        <item name="colorAccent">?msColorAccent</item>
    </style>

    <style name="MaterialSettingsThemeDark" parent="Theme.AppCompat.NoActionBar">
        <item name="msColorPrimary">@color/ms_black</item>
        <item name="msColorPrimaryDark">@color/ms_black</item>
        <item name="msColorAccent">@color/ms_black</item>
        <item name="msWindowBg">@color/ms_dark_window_bg</item>
        <item name="msToolbarTheme">@style/MaterialSettingsTheme_Toolbar</item>
        <item name="msToolbarThemePopup">@style/MaterialSettingsTheme_Toolbar_PopupDark</item>
        <item name="msDivider">@style/MaterialSettingsTheme_DividerDark</item>
        <item name="msColorHeader">@color/ms_white</item>
        <item name="msColorTextTitle">@color/ms_white</item>
        <item name="msColorTextSubTitle">@color/ms_white</item>
        <item name="msTextStyle">@style/MaterialSettingsTheme_Text</item>
        <item name="msSubTextStyle">@style/MaterialSettingsTheme_SubText</item>
        <item name="msHeaderStyle">@style/MaterialSettingsTheme_Header</item>
        <item name="msItem">@style/MaterialSettingsTheme_ItemDark</item>
        <item name="android:windowBackground">?msWindowBg</item>
        <item name="colorPrimary">?msColorPrimary</item>
        <item name="colorPrimaryDark">?msColorPrimaryDark</item>
        <item name="colorAccent">?msColorAccent</item>
    </style>

    <style name="MaterialSettingsTheme_Toolbar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="MaterialSettingsTheme_Toolbar_Popup" parent="@style/ThemeOverlay.AppCompat.Light" />

    <style name="MaterialSettingsTheme_Toolbar_PopupDark" parent="@style/ThemeOverlay.AppCompat.Dark" />

    <style name="MaterialSettingsTheme_Item" parent="@style/Settings_Item" />

    <style name="MaterialSettingsTheme_ItemDark" parent="@style/Settings_Item">
        <item name="android:background">@drawable/default_click_bg_dark</item>
    </style>

    <style name="MaterialSettingsTheme_Divider" parent="@style/Settings_Item_Divider" />

    <style name="MaterialSettingsTheme_DividerDark" parent="@style/Settings_Item_Divider">
        <item name="android:background">@drawable/default_layout_divider_dark</item>
    </style>

    <style name="MaterialSettingsTheme_Text" parent="@style/Settings_Item_Text" />

    <style name="MaterialSettingsTheme_SubText" parent="@style/Settings_Item_SubText" />

    <style name="MaterialSettingsTheme_Header" parent="@style/Settings_Header" />

    <style name="Settings_Header">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/activity_settings_item_padding</item>
        <item name="android:paddingRight">@dimen/activity_settings_item_padding</item>
        <item name="android:paddingTop">@dimen/activity_settings_item_padding</item>
        <item name="android:textColor">?msColorHeader</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Settings_Item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/activity_settings_item_padding</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/default_click_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="autocheck">true</item>
    </style>

    <style name="Settings_Item_Text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textColor">?msColorTextTitle</item>
    </style>

    <style name="Settings_Item_SubText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textColor">?msColorTextSubTitle</item>
    </style>

    <style name="Settings_Item_Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/default_layout_divider</item>
    </style>

    <style name="Settings_Item_CheckBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MaterialSettings.Red" parent="MaterialSettingsTheme">
        <item name="msColorPrimary">@color/redPrimary</item>
        <item name="msColorPrimaryDark">@color/redPrimaryDark</item>
        <item name="msColorAccent">@color/redAccent</item>
        <item name="msColorHeader">@color/redAccent</item>
    </style>

    <style name="MaterialSettings.Dark" parent="MaterialSettingsThemeDark">
        <!--
        <item name="msToolbarTheme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
        <item name="msToolbarThemePopup">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="msColorPrimary">@color/darkPrimary</item>
        <item name="msColorPrimaryDark">@color/darkPrimaryDark</item>
        <item name="msColorAccent">@color/darkAccent</item>
        <item name="msWindowBg">@color/darkBg</item>
